{
  "expo": {
    "name": "VersoVivo",
    "slug": "versovivo",
    "version": "0.1.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#0A0A0A"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.versovivo.app"
    },
    "android": {
      "package": "com.versovivo.app",
      "adaptiveIcon": {
        "backgroundColor": "#0A0A0A"
      }
    },
    "web": {
      "bundler": "metro"
    }
  }
}
