{
  "expo": {
    "name": "VersoVivo",
    "slug": "versovivo",
    "version": "0.1.0",
    "orientation": "portrait",
    "userInterfaceStyle": "dark",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#0A0A0A"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.versovivo.app",
      "config": {
        "googleSignIn": {
          "reservedClientId": "YOUR_REVERSED_CLIENT_ID"
        }
      },
      "services": [
        ["com.apple.developer.applesignin"]
      ]
    },
    "android": {
      "package": "com.versovivo.app",
      "adaptiveIcon": {
        "backgroundColor": "#0A0A0A"
      }
    },
    "plugins": [
      "expo-apple-authentication"
    ]
  }
}
